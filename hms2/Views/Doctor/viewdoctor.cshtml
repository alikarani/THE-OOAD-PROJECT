@model List<hms2.Models.Adddoctormodel>
@{
    ViewBag.Title = "viewdoctor";
}
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>SID</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>qualification</th>
                        <th>Cellnum</th>
                        <th>CNIC</th>
                        <th>Gender</th>
                        <th>Nationality</th>


                    </tr>
                </thead>

                <tbody>

                    @foreach (var item in Model)
            {
                        <tr>
                            <td>@item.SID</td>
                            <td>@item.firstName </td>
                            <td>@item.lastName  </td>
                            <td> @item.qualification </td>
                            <td>  @item.cellnum</td>
                            <td> @item.cnic </td>
                            <td> @item.gender </td>
                            <td> @item.nationality </td>
                            <td>@Html.ActionLink("update", "updatedoctor", new { SID = @item.SID }, new { @class = "btn btn-primary" })</td>
                            <td>@Html.ActionLink("Delete", "Deletedoctor", new { SID = @item.SID }, new { @class = "btn btn-primary" })</td>


                        </tr>
                    }


                </tbody>
            </table>
        </div>






